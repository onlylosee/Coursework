<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#3047D4"/>
            <corners android:radius="10dp"/>
            <stroke android:color="#2D46A6" android:width="3dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#2236B4"/>
            <corners android:radius="10dp"/>
            <stroke android:color="#1E2F80" android:width="3dp"/>
        </shape>
    </item>
</selector>